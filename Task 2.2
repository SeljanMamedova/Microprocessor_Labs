void setup() {
  Serial.begin(9600);            
  uint8_t result;                   // Create an 8-bit variable in RAM to hold the final result.

  asm volatile(
   
    "ldi r16, 5      \n\t"       // Load the value 5 into register r16.
    "ldi r17, 8      \n\t"      // Load the value 8 into register r17.
    "mov r18, r16    \n\t"      // r18 = 5.
    "add r18, r17    \n\t"      // Add r17 to r18 
    "sts %[res], r18 \n\t"      // store r18 into result variable
    : [res] "=m" (result)       // output
    :                          // no input
    : "r16", "r17", "r18"       
  );
  

  Serial.print("Result (R18) = ");
  Serial.println(result);       // should display 13
}

void loop() {}                  // runs once and stops
